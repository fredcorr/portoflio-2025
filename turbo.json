{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [],
  "globalEnv": [
    "ALLOW_CRAWLERS",
    "NEXT_PUBLIC_CONTACT_EMAIL",
    "NEXT_PUBLIC_RECAPTCHA_SITE_KEY",
    "RECAPTCHA_SECRET_KEY",
    "MAIL_APP_ENDPOINT",
    "SUBMISSION_SECRET",
    "SITE_URL",
    "SANITY_PROJECT_ID",
    "SANITY_DATASET",
    "SANITY_API_READ_TOKEN",
    "SANITY_STUDIO_PROJECT_ID",
    "SANITY_STUDIO_DATASET",
    "SANITY_STUDIO_PREVIEW_URL",
    "SANITY_STUDIO_PREVIEW_SECRET",
    "SUBMISSION_SECRET",
    "MAIL_APP_ENDPOINT"
  ],
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "dist/**"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ],
      "outputs": []
    },
    "format": {
      "outputs": []
    },
    "format:check": {
      "outputs": []
    },
    "deploy": {
      "dependsOn": [
        "build"
      ],
      "outputs": []
    }
  }
}
